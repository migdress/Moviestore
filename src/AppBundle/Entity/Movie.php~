<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;


/**
 * @ORM\Entity
 * @ORM\Table(name="movie")
 */
class Movie {
	
	/**
	 * @ORM\Column(type="integer")
	 * @ORM\Id
	 */
	private $movie_id;
	
	/**
	 * @ORM\Column(type="integer")
	 */
	private $genre_id;
	
	/**
	 * @ORM\Column(type="string",length=30)
	 */
	private $movie_name;
	
	/**
	 * @ORM\Column(type="string",length=150)
	 */
	private $movie_desc;
	
}