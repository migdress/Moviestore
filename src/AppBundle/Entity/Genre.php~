<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;


/**
 * @ORM\Entity
 * @ORM\Table(name="genre")
 */
class Genre {
	/**
	 * @ORM\Column(type="integer")
	 * @ORM\Id
	 */
	private $genre_id;
	
	/**
	 * @ORM\Column(type="string", length=30)
	 */
	private $genre_name;
	
}